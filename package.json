{
  "name": "reduxtoolkitwithmock",
  "version": "1.0.0",
  "private": true,
  "description": "concepts-optimistic-updates",
  "keywords": [],
  "main": "./src/index.tsx",
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -rf dist/",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --colors --silent"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "dependencies": {
    "@hookform/devtools": "^3.0.2",
    "@hookform/resolvers": "^2.4.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.4",
    "@mswjs/data": "^0.3.0",
    "@reduxjs/toolkit": "^1.6.0-rc.1",
    "axios": "^0.21.4",
    "msw": "0.28.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-icons": "3.11.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.2",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^6.0.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.6",
    "node-sass": "4.14.1",
    "typescript": "~4.2.4",
    "whatwg-fetch": "^3.4.1",
    "redux-mock-store": "^1.5.4"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coveragePathIgnorePatterns": [
      "src/mocks",
      "src/react-app-env.d.ts",
      "src/setupTests.ts",
      "src/index.tsx"
    ]
  }
}
